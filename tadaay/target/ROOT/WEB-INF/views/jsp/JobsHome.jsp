<script src="<%=request.getContextPath()%>/resources/js/jquery.circliful.min.js"></script>
<script>
var acc = document.getElementsByClassName("accordion");
var i;
for (i = 0; i < acc.length; i++) {
    acc[i].onclick = function(){
        this.classList.toggle("active");
        this.nextElementSibling.classList.toggle("show-in");
        this.nextElementSibling.classList.toggle("show");
  }
}
$( document ).ready(function() { // 6,32 5,38 2,34
	var awscoursecount = 500;
	var devopscoursecount = 100;
	var javacount = 300;
	 $("#course-circle1").circliful({
		 animation:0,
	       animationStep: 1,
	       start: 2,
	       backgroundBorderWidth: 20,
	        foregroundBorderWidth: 20,
	        backgroundColor: "#eee",
	        foregroundColor: '#03a9f4',
	        fontColor: '#fff',
	        fillColor: '#03a9f4b0',
	        noPercentageSign: true,
	        percent: (awscoursecount)
	    });

    $("#course-circle2").circliful({
    	 animation:0,
        animationStep: 6,
        backgroundBorderWidth: 20,
        foregroundBorderWidth: 20,
        backgroundColor: "#eee",
        foregroundColor: '#c06',
        fontColor: '#fff',
        fillColor: '#cc0066b0',
        noPercentageSign: true,
        percent: (devopscoursecount)
    });
    $("#course-circle3").circliful({
    	 animation:0,
        animationStep: 6,
        backgroundBorderWidth: 20,
        foregroundBorderWidth: 20,
        backgroundColor: "#eee",
        foregroundColor: '#d2590d',
        fontColor: '#fff',
        fillColor: '#ff6a00b8',
        noPercentageSign: true,
        percent: (javacount)
    });
});
</script>
